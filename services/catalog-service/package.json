{
  "name": "catalog-service",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --testPathPattern=\"unit/\" --verbose",
    "test:unit": "jest --testPathPattern=\"unit/\" --verbose",
    "test:integration": "jest --testPathPattern=\"integration/\" --verbose",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "PRISMA_CLI=true npx prisma generate",
    "prisma:generate:win": "set PRISMA_CLI=true && npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@prisma/client": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^5.1.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "pg": "^8.16.3",
    "prisma": "^5.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
